<%- include('header'); -%>
<div class="panel panel-headline">
	<div class="panel-heading">
		<h3 class="panel-title">Tổng kết tuần</h3>
		<p class="panel-subtitle">Period: Ngày 14, tháng 11, 2016 - Ngày 21, tháng 11, 2016</p>
	</div>
	<div class="panel-body">
		<div class="row">
			<div class="col-md-3">
				<div class="metric">
					<span class="icon"><i class="fa fa-download"></i></span>
					<p>
						<span class="number">1,252</span>
						<span class="title">Lượt tải</span>
					</p>
				</div>
			</div>
			<div class="col-md-3">
				<div class="metric">
					<span class="icon"><i class="fa fa-shopping-bag"></i></span>
					<p>
						<span class="number">203</span>
						<span class="title">Lượt mua</span>
					</p>
				</div>
			</div>
			<div class="col-md-3">
				<div class="metric">
					<span class="icon"><i class="fa fa-eye"></i></span>
					<p>
						<span class="number">274,678</span>
						<span class="title">Lần ghé thăm</span>
					</p>
				</div>
			</div>
			<div class="col-md-3">
				<div class="metric">
					<span class="icon"><i class="fa fa-bar-chart"></i></span>
					<p>
						<span class="number">35%</span>
						<span class="title">Doang số tăng</span>
					</p>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="col-md-9">
				<div id="headline-chart" class="ct-chart"></div>
			</div>
			<div class="col-md-3">
				<div class="weekly-summary text-right">
					<span class="number">2,315</span> <span class="percentage"><i class="fa fa-caret-up text-success"></i> 12%</span>
					<span class="info-label">Tổng lượt bán</span>
				</div>
				<div class="weekly-summary text-right">
					<span class="number">$900.000</span> <span class="percentage"><i class="fa fa-caret-up text-success"></i> 23%</span>
					<span class="info-label">Doanh thu hàng tháng</span>
				</div>
				<div class="weekly-summary text-right">
					<span class="number">$65.000.938</span> <span class="percentage"><i class="fa fa-caret-down text-danger"></i> 8%</span>
					<span class="info-label">Tổng doanh thu</span>
				</div>
			</div>
		</div>
	</div>
</div>

<div class="row">
	<div class="col-md-4">
		<!-- VISIT CHART -->
		<div class="panel">
			<div class="panel-heading">
				<h3 class="panel-title">Lượt ghé thăm</h3>
				<div class="right">
					<button type="button" class="btn-toggle-collapse"><i class="lnr lnr-chevron-up"></i></button>
					<button type="button" class="btn-remove"><i class="lnr lnr-cross"></i></button>
				</div>
			</div>
			<div class="panel-body">
				<div id="visits-chart" class="ct-chart"></div>
			</div>
		</div>
		<!-- END VISIT CHART -->
	</div>
	<div class="col-md-4">
		<!-- REALTIME CHART -->
		<div class="panel">
			<div class="panel-heading">
				<h3 class="panel-title">Tiến trình hệ thống</h3>
				<div class="right">
					<button type="button" class="btn-toggle-collapse"><i class="lnr lnr-chevron-up"></i></button>
					<button type="button" class="btn-remove"><i class="lnr lnr-cross"></i></button>
				</div>
			</div>
			<div class="panel-body">
				<div id="system-load" class="easy-pie-chart" data-percent="70">
					<span class="percent">70</span>
				</div>
				<h4>CPU Load</h4>
				<ul class="list-unstyled list-justify">
					<li>High: <span>95%</span></li>
					<li>Average: <span>87%</span></li>
					<li>Low: <span>20%</span></li>
					<li>Threads: <span>996</span></li>
					<li>Processes: <span>259</span></li>
				</ul>
			</div>
		</div>
		<!-- END REALTIME CHART -->
	</div>
	<div class="col-md-4">
		<!-- TASKS -->
		<div class="panel">
			<div class="panel-heading">
				<h3 class="panel-title">Quản lý hệ thống gian hàng</h3>
				<div class="right">
					<button type="button" class="btn-toggle-collapse"><i class="lnr lnr-chevron-up"></i></button>
					<button type="button" class="btn-remove"><i class="lnr lnr-cross"></i></button>
				</div>
			</div>
			<div class="panel-body">
				<ul class="list-unstyled task-list">
					<li>
						<p>Tiến trình cải tiến<span class="label-percent">23%</span></p>
						<div class="progress progress-xs">
							<div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="23" aria-valuemin="0" aria-valuemax="100" style="width:23%">
								<span class="sr-only">23% Complete</span>
							</div>
						</div>
					</li>
					<li>
						<p>Tải &amp; Upload nhà <span class="label-percent">80%</span></p>
						<div class="progress progress-xs">
							<div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" style="width: 80%">
								<span class="sr-only">80% Complete</span>
							</div>
						</div>
					</li>
					<li>
						<p>Kiểm tra gian hàng<span class="label-percent">100%</span></p>
						<div class="progress progress-xs">
							<div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100" style="width: 100%">
								<span class="sr-only">Success</span>
							</div>
						</div>
					</li>
					<li>
						<p>Kiểm tra server<span class="label-percent">45%</span></p>
						<div class="progress progress-xs">
							<div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 45%">
								<span class="sr-only">45% Complete</span>
							</div>
						</div>
					</li>
					<li>
						<p>Sức chứa gian hàng <span class="label-percent">10%</span></p>
						<div class="progress progress-xs">
							<div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100" style="width: 10%">
								<span class="sr-only">10% Complete</span>
							</div>
						</div>
					</li>
				</ul>
			</div>
		</div>
		<!-- END TASKS -->
	</div>
</div>
<%- include('footer'); -%>